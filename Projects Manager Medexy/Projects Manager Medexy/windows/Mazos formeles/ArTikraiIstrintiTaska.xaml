<Window x:Class="Projects_Manager_Medexy.ArTikraiIstrintiTaska"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Projects_Manager_Medexy"
        mc:Ignorable="d"
        Title="Ar ištrinti veiksmą?" Height="115" Width="220" WindowStartupLocation="CenterScreen">
    <Grid>
        <Label Content="Ar tikrai norite ištrinti veiksmą??" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" Width="172" Height="28" RenderTransformOrigin="0.5,0.5">
            <Label.RenderTransform>
                <TransformGroup>
                    <ScaleTransform/>
                    <SkewTransform AngleX="-0.546"/>
                    <RotateTransform/>
                    <TranslateTransform X="-0.324"/>
                </TransformGroup>
            </Label.RenderTransform>
        </Label>
        <Button x:Name="SaveBtn" Content="Taip" HorizontalAlignment="Left" Margin="10,43,0,0" VerticalAlignment="Top" Width="86" Height="32" Click="SaveBtn_Click"/>
        <Button x:Name="CancelBtn" Content="Ne" HorizontalAlignment="Left" Margin="107,43,0,0" VerticalAlignment="Top" Width="86" Height="32" Click="CancelBtn_Click"/>

    </Grid>
</Window>
